{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "pair.html",
      "use": "@vercel/static"
    },
    {
      "src": "pair.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/code/(.*)",
      "dest": "pair.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "pair.html"
    }
  ]
}
